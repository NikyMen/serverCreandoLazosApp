{
  "name": "creando-lazos-server",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "cross-env DOTENV_CONFIG_PATH=../.env tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "node ./node_modules/typescript/lib/tsc.js -p tsconfig.json",
    "postbuild": "node scripts/copy-prisma.js",
    "prisma:migrate": "dotenv -e ../.env prisma migrate dev --name init",
    "prisma:generate": "dotenv -e ../.env prisma generate",
    "prisma:seed": "dotenv -e ../.env tsx prisma/seed.ts",
    "prisma:generate:ci": "npx prisma generate",
    "prisma:deploy": "npx prisma migrate deploy",
    "prisma:push": "npx prisma db push --accept-data-loss",
    "prisma:seed:ci": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "serve-static": "^1.15.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.4",
    "prisma": "^5.18.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.18.0 <21.0.0"
  }
}
