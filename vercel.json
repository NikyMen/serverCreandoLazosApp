{
  "version": 2,
  "installCommand": "npm install",
  "buildCommand": "npx prisma generate && npm run build",
  "functions": {
    "api/index.ts": {
      "runtime": "nodejs20.x",
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "/api/index" }
  ]
}